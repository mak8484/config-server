{
    "pageId": "submit-csc",
    "updateStateValues": [
      {
        "key": "$.claim.FF_FLATFROG",
        "value": "$.FF_FLATFROG"
      },
      {
        "key": "$.claim.TP_CONTAINER",
        "value": "$.TP_CONTAINER"
      },
      {
        "key": "$.claim.poiRepairShop",
        "value": "$.poiRepairShop"
      }
    ],
    "nxNextText": "submit.nextText",
    "dynamicFormPageConfig": {
        "titleConfig": [
            {
                "id": "SUBMIT_TITLE",
                "headerOption": "page",
                "headerType": "h1",
                "label": "submit-csc.titleText",
                "type": "HEADLINE"
            },
            {
                "id": "SUBMIT_SUB_TITLE",
                "headerOption": "subsection-small",
                "headerType": "h2",
                "label": "submit-csc.claimDetailsTitle",
                "type": "HEADLINE"
            },
            {
                "id": "SUB_HEADLINE_MOTOR",
                "label": "submit.motorVehicleInsuranceClaim",
                "type": "HEADLINE",
                "headerOption": "subsection-xsmall",
                "headerType": "h2"
            }
        ],
        "formFields": [
            {
                "id": "SUB_CLAIM_DATA",
                "label": "<strong>Vehicle:</strong><br>%jsonPath%$.claim.NP_VEHICLE_SELECTED.label%jsonPath%<br>%jsonPath%$.claim.DAMAGE_TYPE%jsonPath% - %jsonPath%$.claim.DAMAGE_SUB_TYPE%jsonPath% - %jsonPath%$.claim.VC_FTGW_TYPES%jsonPath%<br>%jsonPath%$.claim.AYI_DATE_DISPLAY%jsonPath%",
                "type": "PARAGRAPH",
                "showComponentConditions": [
                    {
                        "expression": "{$.claim.VC_FTGW_TYPES}"
                    }
                ]
            },
            {
                "id": "SUB_CLAIM_DATA",
                "label": "<strong>Vehicle:</strong><br>%jsonPath%$.claim.NP_VEHICLE_SELECTED.label%jsonPath%<br>%jsonPath%$.claim.DAMAGE_TYPE%jsonPath% - %jsonPath%$.claim.CWH_COLLISION_WITH_HUMAN%jsonPath%<br>%jsonPath%$.claim.AYI_DATE_DISPLAY%jsonPath%",
                "type": "PARAGRAPH",
                "showComponentConditions": [
                    {
                        "expression": "{$.claim.DAMAGE_TYPE} == 'COLLISION_HUMAN'"
                    }
                ]
            },
            {
                "id": "SUB_CLAIM_DATA",
                "label": "<strong>Vehicle:</strong><br>%jsonPath%$.claim.NP_VEHICLE_SELECTED.label%jsonPath%<br>%jsonPath%$.claim.DAMAGE_TYPE%jsonPath% - %jsonPath%$.claim.DAMAGE_SUB_TYPE%jsonPath%<br>%jsonPath%$.claim.AYI_DATE_DISPLAY%jsonPath%",
                "type": "PARAGRAPH",
                "showComponentConditions": [
                    {
                        "expression": "! {$.claim.VC_FTGW_TYPES} && ! {$.claim.CC_FTGW_TYPES}"
                    },
                    {
                        "expression": "{$.claim.DAMAGE_SUB_TYPE}"
                    },
                    {
                        "expression": "{$.claim.DAMAGE_TYPE} != 'COLLISION_HUMAN'"
                    }
                ]
            },
            {
                "id": "SUB_CLAIM_DATA",
                "label": "<strong>Vehicle:</strong><br>%jsonPath%$.claim.NP_VEHICLE_SELECTED.label%jsonPath%<br>%jsonPath%$.claim.DAMAGE_TYPE%jsonPath%<br>%jsonPath%$.claim.AYI_DATE_DISPLAY%jsonPath%",
                "type": "PARAGRAPH",
                "showComponentConditions": [
                    {
                        "expression": "! {$.claim.VC_FTGW_TYPES} && ! {$.claim.CC_FTGW_TYPES}"
                    },
                    {
                        "expression": "{$.claim.DAMAGE_TYPE} != 'COLLISION_HUMAN'"
                    },
                    {
                        "expression": "! {$.claim.DAMAGE_SUB_TYPE}"
                    }
                ]
            },
            {
                "id": "submit.notifier.contactDetails",
                "type": "HEADLINE",
                "label": "submit.contactDetails",
                "headerOption": "subsection-medium",
                "headerType": "h2"
            },
            {
                "id": "submit.broker.title",
                "type": "HEADLINE",
                "label": "submit.broker",
                "showComponentConditions": [
                    {
                        "expression": "!!{$.claim.SUBMIT_PNR.brokerNotifier}"
                    }
                ],
                "headerOption": "subsection-small",
                "headerType": "h4"
            },
            {
                "label": "common.email",
                "showComponentConditions": [
                    {
                        "expression": "!!{$.claim.SUBMIT_PNR.brokerNotifier}"
                    }
                ],
                "id": "claim.SUBMIT_EMAIL_BROKER",
                "type": "INPUT",
                "inputType": "email",
                "validators": [
                    {
                        "type": "PATTERN",
                        "value": "^[A-Za-z0-9!#$%&'*+\\-\/=?^_`{|}~]+(\\.[A-Za-z0-9!#$%&'*+\\-\/=?^_`{|}~]+)*@[A-Za-z0-9]+(-[A-Za-z0-9]+)*(\\.[A-Za-z0-9]+(-[A-Za-z0-9]+)*)*\\.([A-Za-z0-9]{2,}|[A-Za-z0-9]+(-[A-Za-z0-9]+)+)$",
                        "errorMessage": "common.validation.email"
                    },
                    {
                        "type": "REQUIRED",
                        "errorMessage": "submit.validation.required"
                    }
                ]
            },
            {
                "label": "common.phone_short",
                "showComponentConditions": [
                    {
                        "expression": "!!{$.claim.SUBMIT_PNR.brokerNotifier}"
                    }
                ],
                "id": "claim.SUBMIT_PHONE_BROKER",
                "type": "INPUT",
                "inputType": "tel",
                "validators": [
                    {
                        "type": "PATTERN",
                        "value": "\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*",
                        "errorMessage": "common.validation.phone"
                    },
                    {
                        "type": "REQUIRED",
                        "errorMessage": "submit.validation.required"
                    }
                ]
            },
            {
                "id": "submit.notifier.title",
                "type": "HEADLINE",
                "label": "submit.notifier",
                "headerOption": "subsection-small",
                "headerType": "h4"
            },
            {
                "label": "common.fullName",
                "id": "claim.SUBMIT_FULLNAME",
                "type": "INPUT",
                "inputType": "text",
                "showComponentConditions": [
                     {
                         "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier}"
                     }
                 ],
                "validators": [
                  {
                      "type": "REQUIRED",
                      "errorMessage": "submit-pnr.policyholderName.required"
                  },
                  {
                    "type": "MAX_LENGTH",
                    "value": "60",
                    "errorMessage":  "submit-pnr.policyholderName.max"
                  },
                  {
                      "type": "PATTERN",
                      "value": "^[A-Za-z0-9 .,_'\\-\\(\\)*]{0,99}$",
                      "errorMessage": "common.noSpecialCharacters"
                  }
                ]
            },
            {
                "label": "common.email",
                "id": "claim.SUBMIT_EMAIL",
                "type": "INPUT",
                "inputType": "email",
                "validators": [
                    {
                        "type": "PATTERN",
                        "value": "^[A-Za-z0-9!#$%&'*+\\-\/=?^_`{|}~]+(\\.[A-Za-z0-9!#$%&'*+\\-\/=?^_`{|}~]+)*@[A-Za-z0-9]+(-[A-Za-z0-9]+)*(\\.[A-Za-z0-9]+(-[A-Za-z0-9]+)*)*\\.([A-Za-z0-9]{2,}|[A-Za-z0-9]+(-[A-Za-z0-9]+)+)$",
                        "errorMessage": "common.validation.email"
                    },
                    {
                        "type": "REQUIRED",
                        "errorMessage": "submit.validation.required"
                    }
                ]
            },
            {
                "label": "common.mobile_short",
                "id": "claim.SUBMIT_MOBILE",
                "type": "INPUT",
                "inputType": "tel",
                "validators": [
                    {
                        "type": "PATTERN",
                        "value": "\\s*0\\s*4\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*",
                        "errorMessage": "common.validation.mobile"
                    },
                    {
                        "type": "REQUIRED",
                        "errorMessage": "submit.validation.required"
                    }
                ]
            },
            {
                "label": "submit-csc.preferred",
                "id": "claim.SUBMIT_PREFERRED_CHANNEL",
                "type": "RADIO",
                "horizontalLayout": true,
                "options": [
                    {
                        "value": "SUBMIT_PREFERRED_EMAIL",
                        "label": "common.email_short"
                    },
                    {
                        "value": "SUBMIT_PREFERRED_MOBILE",
                        "label": "common.sms"
                    }
                ],
                "hideComponentConditions": [
                    {
                        "expression": "!!{$.claim.SUBMIT_PNR.brokerNotifier}"
                    }
                ],
                "validators": [
                    {
                        "type": "REQUIRED",
                        "value": "true",
                        "errorMessage": "common.validation.required"
                    }
                ]
            },
            {
                "label": "submit-csc.preferredForBroker",
                "id": "claim.SUBMIT_PREFERRED_CHANNEL",
                "type": "RADIO",
                "horizontalLayout": true,
                "options": [
                    {
                        "value": "SUBMIT_PREFERRED_EMAIL",
                        "label": "common.email_short"
                    },
                    {
                        "value": "SUBMIT_PREFERRED_MOBILE",
                        "label": "common.sms"
                    }
                ],
                "showComponentConditions": [
                    {
                        "expression": "!!{$.claim.SUBMIT_PNR.brokerNotifier}"
                    }
                ],
                "validators": [
                    {
                        "type": "REQUIRED",
                        "value": "true",
                        "errorMessage": "common.validation.required"
                    }
                ]
            },
            {
                "id": "SUB_NOTE",
                "label": "submit-csc.confirmationForBroker",
                "type": "PARAGRAPH",
                "showComponentConditions": [
                    {
                        "expression": "!!{$.claim.SUBMIT_PNR.brokerNotifier}"
                    }
                ]
            },
            {
                "id": "SUB_NOTE",
                "label": "submit-csc.confirmation",
                "type": "PARAGRAPH",
                "showComponentConditions": [
                    {
                        "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier}"
                    }
                ]
            },
            {
              "id": "claim.SUBMIT_PNR.isPolicyHolder",
              "type": "CIRCLE_BUTTONS",
              "label": "submit-pnr.policyholder.title",
              "options": {
                "buttons": [
                  {
                    "id": "POLICY_HOLDER_YES",
                    "icon": "product-icon-yes",
                    "label": ""
                  },
                  {
                    "id": "POLICY_HOLDER_NO",
                    "icon": "product-icon-no",
                    "label": ""
                  }
                ],
                "multiSelection": false
              },
              "validators": [
                {
                  "type": "REQUIRED",
                  "errorMessage": "common.validation.required"
                }
              ],
              "hideComponentConditions": [
                {
                  "expression": "!!{$.claim.SUBMIT_PNR.brokerNotifier}"
                }
              ]
            },
            {
                "id": "submit.notifier.title",
                "type": "HEADLINE",
                "label": "submit-pnr.notifier",
                "headerOption": "subsection-small",
                "headerType": "h4",
                "showComponentConditions": [
                    {
                        "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier} && {$.claim.SUBMIT_PNR.isPolicyHolder} == 'POLICY_HOLDER_NO'"
                    }
                ]
            },
            {
              "label": "submit-pnr.other-relationship",
              "id": "claim.SUBMIT_PNR.relationshipType",
              "type": "DROPDOWN",
              "options": [
                {
                  "label": "Authorised Representative",
                  "value": "AuthRepNotifier"
                },
                {
                  "label": "Nominated Driver",
                  "value": "NomDriverNotifier"
                }
                ,
                {
                  "label": "Other",
                  "value": "OtherNotifier"
                }
              ],
              "validators": [
                {
                  "type": "REQUIRED",
                  "errorMessage": "submit-pnr.ph-relationship.valid"
                }
              ],
              "showComponentConditions": [
                  {
                      "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier} && {$.claim.SUBMIT_PNR.isPolicyHolder} == 'POLICY_HOLDER_NO'"
                  }
              ]
            },
            {
                "id": "claim.SUBMIT_PNR.otherSpecified",
                "label": "submit-pnr.other-specify",
                "type": "TEXT_AREA",
                "maxLength": 198,
                "showComponentConditions": [
                    {
                        "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier} && {$.claim.SUBMIT_PNR.isPolicyHolder} == 'POLICY_HOLDER_NO' && {$.claim.SUBMIT_PNR.relationshipType} == 'OtherNotifier'"
                    }
                ],
                "validators": [
                    {
                        "type": "REQUIRED",
                        "errorMessage": "submit-pnr.ph-relationship.valid"
                    },
                    {
                      "type": "PATTERN",
                      "value": "^[A-Za-z0-9\\s\\r\\n .,&!()?*#@%+$-\\/\\\\:;\\'‘’`\"“”]*$",
                      "errorMessage": "common.noSpecialCharacters"
                    }
                ]
            },
            {
              "label": "submit-pnr.other-name",
              "id": "claim.SUBMIT_PNR.yourFullname",
              "type": "INPUT",
              "inputType": "text",
              "validators": [
                {
                   "type": "REQUIRED",
                   "errorMessage": "submit-pnr.other-name.required"
                },
                {
                  "type": "MAX_LENGTH",
                  "value": "60",
                  "errorMessage":  "submit-pnr.other-name.max"
                },
                {
                  "type": "PATTERN",
                  "value": "^[A-Za-z0-9 .,_'\\-\\(\\)*]{0,99}$",
                  "errorMessage": "common.noSpecialCharacters"
                }
              ],
              "showComponentConditions": [
                {
                  "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier} && {$.claim.SUBMIT_PNR.isPolicyHolder} == 'POLICY_HOLDER_NO'"
                }
              ]
            },
            {
                "label": "submit-pnr.other-email",
                "id": "claim.SUBMIT_PNR.yourEmail",
                "type": "INPUT",
                "inputType": "email",
                "validators": [
                    {
                        "type": "PATTERN",
                        "value": "^[A-Za-z0-9!#$%&'*+\\-\/=?^_`{|}~]+(\\.[A-Za-z0-9!#$%&'*+\\-\/=?^_`{|}~]+)*@[A-Za-z0-9]+(-[A-Za-z0-9]+)*(\\.[A-Za-z0-9]+(-[A-Za-z0-9]+)*)*\\.([A-Za-z0-9]{2,}|[A-Za-z0-9]+(-[A-Za-z0-9]+)+)$",
                        "errorMessage": "common.validation.email"
                    },
                    {
                        "type": "REQUIRED",
                        "errorMessage": "submit.validation.required"
                    }
                ],
                "showComponentConditions": [
                  {
                    "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier} && {$.claim.SUBMIT_PNR.isPolicyHolder} == 'POLICY_HOLDER_NO'"
                  }
                ]
            },
            {
                "label": "submit-pnr.other-mobile",
                "id": "claim.SUBMIT_PNR.yourMobile",
                "type": "INPUT",
                "inputType": "tel",
                "validators": [
                    {
                        "type": "PATTERN",
                        "value": "\\s*0\\s*4\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*\\d\\s*",
                        "errorMessage": "common.validation.mobile"
                    },
                    {
                        "type": "REQUIRED",
                        "errorMessage": "submit.validation.required"
                    }
                ],
                "showComponentConditions": [
                  {
                    "expression": "!{$.claim.SUBMIT_PNR.brokerNotifier} && {$.claim.SUBMIT_PNR.isPolicyHolder} == 'POLICY_HOLDER_NO'"
                  }
                ]
            },
            {
                "id": "claim.submitConfirmTOS",
                "type": "CHECKBOX",
                "disabled": true,
                "label": {
                    "text": "submit.tos.text",
                    "links": [
                        {
                            "label": "submit.tos.linkText",
                            "link": "submit.tos.url",
                            "target": "_blank"
                        }
                    ]
                }
            },
            {
                "id": "claim.SUBMIT_PNR.submitPrivacyCheck",
                "type": "CHECKBOX",
                "label": {
                  "text": "submit-csc.privacyCheck"
                },
                "validators": [
                    {
                        "type": "REQUIRED",
                        "value": "true"
                    }
                ]
            }
        ]
    }
}